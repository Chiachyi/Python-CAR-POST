SOL 200
GEOMCHECK NONE
CEND
DESOBJ(MIN)=1
DISPLACEMENT = ALL
ANALYSIS = STATICS
SUBCASE 1000000
  LABEL = LOAD_T
  SPC = 3
  LOAD = 1
  MPC = 5
  DESSUB = 4
SUBCASE 1000001
  LABEL = LOAD_B
  SPC = 4
  LOAD = 2
  DESSUB = 3
BEGIN BULK
DDVAL   1      -0.7     THRU    2.0     BY      0.1
PARM,OPTEXIT,7
PARM,POST,-1

DOPTPRM,P1,1,P2,5.DESMAX,300

DRESP1,1,m,WEIGHT

$$检查弯曲点
DRESP1,2,DT1,DISP,,,3,,600015
DRESP1,3,DT2,DISP,,,3,,700015
DEQATN  1 BF(X1,X2)=12000./ABS(X1+X2)
DRESP2,4,STIFFB,1
,DRESP1,2,3

DRESP1,5,DT1,DISP,,,3,,600003
DRESP1,6,DT2,DISP,,,3,,700003
$$检查跨距
DEQATN  2 TF(X1,X2)=2000./(ATAN(ABS(X1-X2)911.014)*180./3.141593)
DRESP2,7,STIFFT,2
,DRESP1,5,6

DCONSTR,1,4,14000.,20000.
DCONSTR,2,7,16000.,22000.
DCONADD,3,1
DCONADD,4,2
INCLUDE 'MAT.DAT'
INCLUDE 'DESVAR.DAT'
INCLUDE 'DVPREL1.DAT'
